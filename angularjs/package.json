{
  "name": "angularjs-github-stars",
  "version": "0.0.0",
  "description": "Github Stars with AngularJS",
  "main": "index.js",
  "scripts": {
    "postinstall": "bower install",
    "prestart": "npm install && npm run compress",
    "start": "live-server --watch=dist --port=4200 --host=localhost & npm run watch",
    "clean": "rimraf dist",
    "precompress": "npm run clean && npm test -- --single-run",
    "compress": "gulp compress",
    "watch": "gulp watch",
    "lint": "jshint app && jscs app",
    "pretest": "npm run lint",
    "test": "karma start karma.conf.js",
    "pree2e": "webdriver-manager update",
    "e2e": "protractor protractor.conf.js"
  },
  "keywords": [
    "github",
    "angular"
  ],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "bower": "^1.7.9",
    "chai": "^3.5.0",
    "gulp": "^3.9.1",
    "gulp-angular-templatecache": "^1.8.0",
    "gulp-concat": "^2.6.0",
    "gulp-htmlmin": "^1.3.0",
    "gulp-ng-annotate": "^2.0.0",
    "gulp-uglify": "^1.5.3",
    "jscs": "^2.11.0",
    "jshint": "^2.9.1",
    "karma": "^0.13.22",
    "karma-chai": "^0.1.0",
    "karma-chai-plugins": "^0.7.0",
    "karma-coverage": "^0.5.5",
    "karma-mocha": "^0.2.2",
    "karma-mocha-reporter": "^2.0.0",
    "karma-phantomjs-launcher": "^1.0.0",
    "live-server": "^0.9.2",
    "mocha": "^2.4.5",
    "phantomjs-prebuilt": "^2.1.7",
    "protractor": "^3.2.2",
    "rimraf": "^2.5.2"
  },
  "dependencies": {}
}
